<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="update-timestamp-type-for-created-date-and-updated-date-columns-in-location-table" author="dlly">
        <modifyDataType columnName="CREATED_DATE"
                        newDataType="TIMESTAMP(6)"
                        tableName="LOCATION"/>
        <modifyDataType columnName="UPDATED_DATE"
                        newDataType="TIMESTAMP(6)"
                        tableName="LOCATION"/>
    </changeSet>
</databaseChangeLog>